{
    "name": "micseres/php-instagram-app",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "TestsFunctional\\": "tests/"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-amqp": "*",
        "ext-json": "*",
        "mgp25/instagram-php": "dev-master",
        "monolog/monolog": "^1.24",
        "symfony/console": "^4.2",
        "symfony/dotenv": "^4.2",
        "gepur-it/rabbitmq-bundle": "^2.0",
        "symfony/config": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/framework-bundle": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
